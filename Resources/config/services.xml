<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="msi_google.map.factory.class">Msi\Bundle\GoogleBundle\Map\MapFactory</parameter>
    </parameters>

    <services>
        <service id="msi_google.map.factory" class="%msi_google.map.factory.class%" />

        <service id="msi_google.google.twig.extension" class="Msi\Bundle\GoogleBundle\Twig\Extension\GoogleExtension" public="false">
            <tag name="twig.extension" />
        </service>
    </services>
</container>
